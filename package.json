{
  "name": "vanilla",
  "version": "0.0.1",
  "description": "Front End application",
  "repository": "https://git.otms.cn/opentrans/opentrans",
  "main": "./src/js/app.jsx",
  "scripts": {
    "start:api": "better-npm-run start:api",
    "start:dll": "better-npm-run start:dll",
    "start": "better-npm-run start",
    "build": "better-npm-run build:dll; better-npm-run build"
  },
  "betterScripts": {
    "start:dll": {
      "command": "webpack --config ./config/webpack.dll.babel.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build:dll": {
      "command": "webpack --config ./config/webpack.dll.babel.js",
      "env": {
        "NODE_ENV": "prod"
      }
    },
    "start:api": {
      "command": "nodemon --watch server/ --watch config/ server/",
      "env": {
        "NODE_ENV": "dev",
        "project": "track-trace"
      }
    },
    "start": {
      "command": "webpack-dev-server ",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.17",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "^0.22.0",
    "eslint": "^4.5.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "^11.3.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "^3.1.2",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0",
    "open-browser-webpack-plugin": "0.0.5",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-devtools-extension": "^2.13.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglify-js": "^2.4.16",
    "uglify-loader": "^2.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.1"
  },
