      "script": "start:api",
      "problemMatcher": [
        "$eslint-stylish"
      ],
      "group": {
          "kind": "build",
          "isDefault": true
      }
    },
    {
      "type": "npm",
      "script": "start",
      "problemMatcher": [
        "$eslint-stylish"
      ],
      "group": {
          "kind": "build",
          "isDefault": true
      }
    },
    {
      "type": "npm",
      "script": "build",
      "problemMatcher": [
        "$eslint-stylish"
      ],
      "group": {
          "kind": "build",
          "isDefault": true
      }
    }
  ]
